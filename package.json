    "start": "better-npm-run start",
    "debug": "better-npm-run debug"
  },
  "betterScripts": {
    "start": {
      "command": "node --require @babel/register server.js",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app*",
        "PORT": "8080"
      }
    },
    "debug": {
      "command": "nodemon --require @babel/register --inspect server.js",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app*",
        "PORT": "8080"
      }
    }
  },
